{
    "extends": "google",
    "parser" : "babel-eslint",
    "parserOptions": {
        "ecmaFeatures": {
          "jsx": true,
          "modules": true,
        }
    },
    "env": {
        "browser": true,
        "node": true
    },
    "rules": {
        "new-cap": 1,
        "linebreak-style": 0,
        "no-unused-vars": 0,
        "no-negated-condition": 1,
        "quotes": [1, "single"],
        "strict": [2, "never"],
        "react/jsx-uses-react": 2,
        "react/jsx-uses-vars": 2,
        "react/jsx-curly-spacing": [2, "always"],
        "react/react-in-jsx-scope": 2,
        "no-var": 0,
        "comma-dangle": 0,
        "require-jsdoc": 0
    },
    "plugins": [
    "react"
    ]
}
